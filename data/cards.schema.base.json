{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "id",
    "total_cards_count",
    "number",
    "sort_by",
    "name",
    "image",
    "rarity",
    "set_short_name",
    "series_short_name",
    "illustrators",
    "set_ids",
    "sets",
    "thumb",
    "variant"
  ],
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "number": {
      "type": "number"
    },
    "sort_by": {
      "type": "number"
    },
    "name": {
      "type": "string"
    },
    "image": {
      "type": "string",
      "format": "uri"
    },
    "rarity": {
      "type": "string"
    },
    "set_short_name": {
      "type": "string"
    },
    "series_short_name": {
      "type": "string"
    },
    "illustrators": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    },
    "set_ids": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "minItems": 1
    },
    "sets": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["name", "image"],
        "properties": {
          "name": {
            "type": "string"
          },
          "image": {
            "type": "string",
            "format": "uri"
          }
        },
        "additionalProperties": false
      },
      "minItems": 1
    },
    "thumb": {
      "type": "string",
      "format": "uri"
    },
    "variant": {
      "type": "string"
    }
  },
  "additionalProperties": true
}
